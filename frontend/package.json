{
    "name": "sdocs",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "npx eslint .&& vue-tsc && vite build",
        "lint": "npx eslint .",
        "lint:fix": "npx eslint . --fix",
        "preview": "vite preview",
        "test:ci": "vue-tsc --noEmit&&cross-env CI=true; vitest --coverage --environment jsdom ",
        "test:ui": "vue-tsc --noEmit&&vitest --ui --environment jsdom",
        "coverage": "vitest --coverage --environment jsdom --reporter html",
        "check": "vue-tsc --noEmit",
        "prettier:fix:ts": "prettier --write ./**/*.ts",
        "prettier:fix:vue": "prettier --write ./**/*.vue"
    },
    "dependencies": {
        "@unocss/reset": "^0.54.3",
        "@vueup/vue-quill": "^1.2.0",
        "axios": "^1.4.0",
        "dayjs": "^1.11.9",
        "uuid": "^9.0.0",
        "vue": "^3.3.4",
        "vue-router": "^4.2.4"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.3.3",
        "@types/eslint-config-prettier": "^6.11.0",
        "@types/jsdom": "^21.1.1",
        "@types/uuid": "^9.0.2",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@vitejs/plugin-vue": "^4.3.1",
        "@vitest/coverage-v8": "^0.34.2",
        "@vitest/ui": "^0.34.2",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/test-utils": "^2.4.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.47.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-vue": "^9.17.0",
        "jsdom": "^22.1.0",
        "prettier": "^3.0.2",
        "typescript": "^5.1.6",
        "vite": "^4.4.9",
        "vitest": "^0.34.2",
        "vue-tsc": "^1.8.8"
    }
}
