{
  "name": "sdocs",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test:ci": "vue-tsc --noEmit&&set CI=true&&vitest --environment jsdom",
    "test:ui": "vue-tsc --noEmit&&vitest --ui --environment jsdom",
    "coverage": "vitest run --coverage",
    "check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@unocss/reset": "^0.51.13",
    "@vueup/vue-quill": "^1.2.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "uuid": "^9.0.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@testing-library/vue": "^7.0.0",
    "@types/jsdom": "^21.1.1",
    "@types/uuid": "^9.0.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-c8": "^0.31.4",
    "@vitest/ui": "^0.31.4",
    "@vue/test-utils": "^2.4.0",
    "jsdom": "^22.1.0",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vitest": "^0.31.4",
    "vue-tsc": "^1.8.3"
  }
}
